@model Product[]

@{
    ViewBag.Title = "Product name";
}

<table>
    <thead>
        <tr><th>Product</th><th>Price</th></tr>
    </thead>
    <tbody>
        @foreach (Product item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@($"{item.Price:C2}")</td>
            </tr>
        }
    </tbody>
</table>
